{
    "name": "megaelixir",
    "version": "1.0.1",
    "description": "Un simple bot para manejar los tickets de usuarios.",
    "main": "./dist/Main.js",
    "engines": {
        "node": ">=15.x"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development ts-node ./src/Main.ts",
        "dev:watch": "cross-env NODE_ENV=development nodemon",
        "lint": "npx eslint --ext .ts ./src",
        "lint:fix": "npx eslint --ext .ts ./src --fix",
        "build": "tsc -b",
        "start": "node ./dist/Main.js"
    },
    "author": "",
    "license": "GNU v3",
    "homepage": "https://github.com/StarArmyDev",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DavichoStar/megaelixir"
    },
    "bugs": {
        "url": "https://github.com/DavichoStar/megaelixir/issues"
    },
    "dependencies": {
        "discord.js": "^12.5.1",
        "express": "^4.17.1",
        "html-minifier": "^4.0.0",
        "mongoose": "^6.0.4",
        "mustache": "^4.2.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/html-minifier": "^4.0.0",
        "@types/mustache": "^4.1.1",
        "@types/node": "^14.11.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-promise": "^4.3.1",
        "nodemon": "^2.0.6",
        "prettier": "^2.1.1",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    }
}
